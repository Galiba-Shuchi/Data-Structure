#include <stdio.h>
#define SIZE 100

int stack[SIZE];
int top = -1;

void push(int value) {
    if (top == SIZE - 1) {
        printf("Overflow");
    } else {
        scanf("%d",& value);
        top++;
        stack[top] = value;
        printf("%d pushed", value);
    }
}

void pop() {
    if (top == -1) {
        printf("Underflow");
    } else {
        printf("%d popped", stack[top]);
        top--;
    }
}

void peek() {
    if (top == -1) {
        printf(" empty");
    } else {
        printf("Top element is %d", stack[top]);
    }
}

void display() {
    if (top == -1) {
        printf("empty");
    } else {
        printf("Stack elements: ");
        for (int i = top; i >= 0; i--) {
            printf("%d ", stack[i]);
        }
        printf("\n");
    }
}

int main() {
    int choice, value;
    while (1) {
        printf("\nStack\n");
        printf("1. Push\n2. Pop\n3. Peek\n4. Display\n5. Exit\n");
        printf("Enter your choice: ");
        scanf("%d", &choice);

        switch (choice) {
        case 1:
            printf("Enter value: ");
            scanf("%d", &value);
            push(value);
            break;
        case 2:
            pop();
            break;
        case 3:
            peek();
            break;
        case 4:
            display();
            break;
        case 5:
            return 0;
        default:
            printf("error");
        }
    }
    return 0;
}
